#include<stdio.h>
main()
{
    int mat[20][20],m,n,i,j;
    printf("Enter the order of the matrix\n");
    scanf("%d%d",&m,&n);
    printf("enter the elements of the matrix\n");
    for(i=0;i<m;++i)
     for(j=0;j<n;++j)
      scanf("%d",((mat+i)+j));
    printf("the elementa are\n");
    for(i=0;i<m;++i){
     for(j=0;j<n;++j)
     printf("%d\t",*(*(mat+i)+j));
    printf("\n");
    }
}
